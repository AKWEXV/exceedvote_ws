@(votes: List[models.Vote])<?xml version="1.0" encoding="UTF-8"?>
<myvote>
	<votes>@for(vote <- votes){@for(ballot <- vote.getBallots()){
		<vote>
			<criterion>
				<id>@vote.getCriterion().getId()</id>
			</criterion>
			<contestant>
				<id>@ballot.getContestant().getId()</id>
				<score>@ballot.getScore()</score>
			</contestant>
		</vote>}}
	</votes>
</myvote>